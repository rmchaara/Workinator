<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content">
	
	<TextView android:id="@+id/txtWorkOutName" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_below="@+id/txtSearch"
		android:layout_marginTop="10dp"
		android:textSize="20sp"
		android:text="Name"/>
	<EditText android:id="@+id/edtWorkOutName"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_toRightOf="@+id/txtWorkOutName"
		android:layout_marginLeft="5dp"
		android:singleLine="true"
		android:layout_alignParentRight="true"/>
	<Button android:id="@+id/btnVideo"
		android:layout_below="@+id/txtWorkOutName" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:width="97dp"
		android:height="70dp"
		android:layout_marginTop="15dp" 
		android:text="Add Video"
		android:background="@drawable/custom"/>
	<Button android:id="@+id/btnContinue" 
		android:layout_below="@+id/txtWorkOutName"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:width="97dp"
		android:height="70dp"
		android:layout_toRightOf="@+id/btnVideo"
		android:layout_marginLeft="2dp"
		android:layout_marginTop="15dp"
		android:text="Continue"
		android:background="@drawable/custom"/>
	<Button android:id="@+id/btnCancel" 
		android:layout_below="@+id/txtWorkOutName"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:width="97dp"
		android:height="70dp"
		android:layout_toRightOf="@+id/btnContinue"
		android:layout_marginLeft="2dp"
		android:layout_marginTop="15dp"
		android:text="Cancel"
		android:background="@drawable/custom"/>
</RelativeLayout>
