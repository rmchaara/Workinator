<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content">
	
	<TextView android:id="@+id/lblReturnName" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:textSize="20dp"
		android:text="Name:"/>
	<TextView android:id="@+id/txtReturnName" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_toRightOf="@+id/lblReturnName"
		android:layout_marginLeft="5dp"
		android:textSize="20dp"/>
	<TextView android:id="@+id/lblYoutube" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_below="@+id/lblReturnName"
		android:textSize="20dp"
		android:text="Youtube:"/>
	<TextView android:id="@+id/txtYoutube" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_below="@+id/lblReturnName"
		android:layout_toRightOf="@+id/lblYoutube"
		android:layout_marginLeft="5dp"
		android:autoLink="web"
		android:singleLine="true"
		android:textSize="20dp"/>
	<TextView android:id="@+id/lblReturnRating" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_below="@+id/lblYoutube"
		android:textSize="20dp"
		android:text="Rating:"/>
	<TextView android:id="@+id/txtReturnRating" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_below="@+id/lblYoutube"
		android:layout_toRightOf="@+id/lblReturnRating"
		android:layout_marginLeft="5dp"
		android:textSize="20dp"/>
	<TextView android:id="@+id/lblDescription" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:layout_below="@+id/lblReturnRating"
		android:textSize="20dp"
		android:text="Workout Routine:"/>
	<ScrollView android:id="@+id/srclReturnWO" 
		android:layout_width="300sp" 
		android:layout_height="150sp"
		android:layout_below="@+id/lblDescription"
		android:layout_centerHorizontal="true"
		android:background="@drawable/textlines">
		<TextView android:id="@+id/txtDescription" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" 
			android:textSize="15dp"/>
	</ScrollView>	
	<Button android:id="@+id/btnRate"
		android:layout_below="@+id/srclReturnWO" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:width="97dp"
		android:height="70dp"
		android:layout_marginTop="15dp" 
		android:layout_marginLeft="50dp"
		android:text="Rate"
		android:background="@drawable/custom"/>
	<Button android:id="@+id/btnReturnClose" 
		android:layout_below="@+id/srclReturnWO"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:width="97dp"
		android:height="70dp"
		android:layout_toRightOf="@+id/btnRate"
		android:layout_marginLeft="2dp"
		android:layout_marginTop="15dp"
		android:text="Close"
		android:background="@drawable/custom"/>
</RelativeLayout>
